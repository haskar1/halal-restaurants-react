{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/api/*": ["src/app/pages/api/*"],
      "@/components/*": ["./src/app/components/*"],
      "@/lib/*": ["./src/app/lib/*"],
      "@/utils/*": ["./src/app/utils/*"],
      "@/stylesheets/*": ["./src/app/stylesheets/*"],
      "@react-leaflet/core": ["./node_modules/@react-leaflet/core"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/dashboard/restaurants/[restaurant_tag]",
    "src/app/search/page.jsx",
    "src/app/components/DashboardNav.jsx",
    "src/app/dashboard/page.jsx",
    "src/app/dashboard/layout.jsx",
    "src/app/dashboard/restaurants/page.jsx",
    "src/app/dashboard/restaurants/create/page.jsx",
    "src/app/dashboard/cuisines/page.jsx",
    "src/app/dashboard/cuisines/create/page.jsx",
    "src/app/dashboard/cuisines/[id]/page.jsx",
    "src/app/components/Sidebar.tsx",
    "src/app/components/SearchResultsList.jsx",
    "src/app/components/SearchResultsFilters.jsx",
    "src/app/components/Map-maplibre.jsx",
    "src/app/components/RestaurantSearch.jsx",
    "src/app/pages/api/get-cuisines/route.js",
    "src/app/pages/api/get-cuisine/route.js",
    "src/app/components/BottomModalSheet.jsx"
  ],
  "exclude": ["node_modules"]
}
